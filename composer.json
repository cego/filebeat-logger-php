{
    "name": "cego/filebeat-logger",
    "description": "Package for logging php enriched json lines that filebeat can collect",
    "type": "project",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.4|^8.0",
        "monolog/monolog": "^2.1.1",
        "ua-parser/uap-php": "^v3.9.14"
    },
    "autoload": {
        "psr-4": {
            "Cego\\": "src/"
        }
    },
    "require-dev": {
        "cego/php-cs-fixer": "^1.0.0",
        "phpstan/phpstan": "^1.0.0"
    }
}
